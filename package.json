    "mocha": "^3.0.2",
    "shortid": "^2.2.8",
    "wc-e2e-page-objects": "^0.6.0",
    "wp-e2e-webdriver": "^0.13.0"
  },
  "engines": {
    "node": ">=0.8.0",
    "npm": ">=1.1.0"
  }
